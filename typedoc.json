{
  "$schema": "https://typedoc.org/schema.json",
  "includeVersion": true,
  "entryPointStrategy": "packages",
  "entryPoints": ["indulgent"],
  "packageOptions": {
    "entryPoints": ["src/**/index.ts"]
  },
  "excludePrivate": true,
  "exclude": ["**/*.test.ts"],
  "plugin": ["typedoc-github-theme"],
  "theme": "typedoc-github-theme",
  "pretty": true
}
